<?xml version="1.0" encoding="utf-8" ?>
<fluidity_options>
    <simulation_name>
        <string_value lines="1">rad_1d_3grp_hom_inf_eig</string_value>
        <comment>Radiation problem:&#x0A;- 1 dim&#x0A;- 3 group&#x0A;- 1 material (homogeneous)&#x0A;- all reflective (infinite)&#x0A;- eigenvalue run</comment>
    </simulation_name>
    <problem_type>
        <string_value lines="1">fluids</string_value>
    </problem_type>
    <geometry>
        <dimension>
            <integer_value rank="0">1</integer_value>
        </dimension>
        <mesh name="CoordinateMesh">
            <from_file file_name="rad_1d_3grp_hom_inf_eig">
                <format name="triangle"></format>
                <stat>
                    <include_in_stat></include_in_stat>
                </stat>
            </from_file>
        </mesh>
        <mesh name="VelocityMesh">
            <from_mesh>
                <mesh name="CoordinateMesh"></mesh>
                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_mesh>
        </mesh>
        <mesh name="ParticleMesh">
            <from_mesh>
                <mesh name="CoordinateMesh"></mesh>
                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_mesh>
        </mesh>
        <mesh name="ParticleMaterialMesh">
            <from_mesh>
                <mesh name="CoordinateMesh"></mesh>
                <mesh_shape>
                    <polynomial_degree>
                        <integer_value rank="0">0</integer_value>
                    </polynomial_degree>
                    <element_type>
                        <string_value>discontinuous lagrangian</string_value>
                    </element_type>
                </mesh_shape>
                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_mesh>
        </mesh>
        <quadrature>
            <degree>
                <integer_value rank="0">3</integer_value>
            </degree>
        </quadrature>
    </geometry>
    <io>
        <dump_format>
            <string_value>vtk</string_value>
        </dump_format>
        <dump_period>
            <constant>
                <real_value rank="0">1</real_value>
            </constant>
        </dump_period>
        <output_mesh name="CoordinateMesh"></output_mesh>
        <stat></stat>
    </io>
    <timestepping>
        <current_time>
            <real_value rank="0">0</real_value>
        </current_time>
        <timestep>
            <real_value rank="0">1</real_value>
        </timestep>
        <finish_time>
            <real_value rank="0">1</real_value>
        </finish_time>
        <final_timestep>
            <integer_value rank="0">0</integer_value>
        </final_timestep>
    </timestepping>
    <material_phase name="dummy">
        <vector_field name="Velocity" rank="1">
            <prescribed>
                <mesh name="VelocityMesh"></mesh>
                <value name="WholeMesh">
                    <constant>
                        <real_value rank="1" shape="1" dim1="dim">0</real_value>
                    </constant>
                </value>
                <output></output>
                <stat>
                    <include_in_stat></include_in_stat>
                </stat>
                <detectors>
                    <exclude_from_detectors></exclude_from_detectors>
                </detectors>
            </prescribed>
        </vector_field>
    </material_phase>
    <embedded_models>
        <radiation>
            <particle_type name="Neutron">
                <equation name="Eigenvalue">
                    <power_iteration>
                        <maximum>
                            <integer_value rank="0">100</integer_value>
                        </maximum>
                        <flux_tolerance_absolute>
                            <real_value rank="0">1e-06</real_value>
                        </flux_tolerance_absolute>
                        <keff_tolerance_relative>
                            <real_value rank="0">1e-06</real_value>
                        </keff_tolerance_relative>
                    </power_iteration>
                    <flux_normalisation name="TotalFlux">
                        <value>
                            <real_value rank="0">1</real_value>
                        </value>
                    </flux_normalisation>
                </equation>
                <energy_discretisation name="WholeSpatialMesh">
                    <continuity>
                        <string_value>discontinuous</string_value>
                    </continuity>
                    <degree>
                        <integer_value rank="0">0</integer_value>
                    </degree>
                    <energy_group_set name="all">
                        <number_of_energy_groups>
                            <integer_value rank="0">3</integer_value>
                        </number_of_energy_groups>
                        <angular_discretisation name="WholeSpatialMesh">
                            <method name="SphericalHarmonic">
                                <parity name="Even">
                                    <exclude_odd_parity_time_source></exclude_odd_parity_time_source>
                                    <angular_moment_set name="AllMoments">
                                        <number_of_angular_moments>
                                            <integer_value rank="0">1</integer_value>
                                        </number_of_angular_moments>
                                        <mesh name="ParticleMaterialMesh"></mesh>
                                        <tensor_field name="Diffusivity">
                                            <diagnostic>
                                                <algorithm name="Internal"></algorithm>
                                                <output>
                                                    <exclude_from_vtu></exclude_from_vtu>
                                                </output>
                                                <stat>
                                                    <exclude_from_stat></exclude_from_stat>
                                                </stat>
                                                <detectors>
                                                    <exclude_from_detectors></exclude_from_detectors>
                                                </detectors>
                                            </diagnostic>
                                        </tensor_field>
                                        <scalar_field name="Absorption">
                                            <diagnostic>
                                                <algorithm name="Internal"></algorithm>
                                                <output>
                                                    <exclude_from_vtu></exclude_from_vtu>
                                                </output>
                                                <stat>
                                                    <exclude_from_stat></exclude_from_stat>
                                                </stat>
                                                <detectors>
                                                    <exclude_from_detectors></exclude_from_detectors>
                                                </detectors>
                                            </diagnostic>
                                        </scalar_field>
                                        <scalar_field name="ParticleFlux">
                                            <prognostic>
                                                <mesh name="ParticleMesh"></mesh>
                                                <equation name="AdvectionDiffusion"></equation>
                                                <spatial_discretisation>
                                                    <continuous_galerkin>
                                                        <advection_terms>
                                                            <exclude_advection_terms></exclude_advection_terms>
                                                        </advection_terms>
                                                        <mass_terms>
                                                            <exclude_mass_terms></exclude_mass_terms>
                                                        </mass_terms>
                                                    </continuous_galerkin>
                                                    <conservative_advection>
                                                        <real_value rank="0">1</real_value>
                                                    </conservative_advection>
                                                </spatial_discretisation>
                                                <temporal_discretisation>
                                                    <theta>
                                                        <real_value rank="0">1</real_value>
                                                    </theta>
                                                </temporal_discretisation>
                                                <solver>
                                                    <iterative_method name="cg"></iterative_method>
                                                    <preconditioner name="sor"></preconditioner>
                                                    <relative_error>
                                                        <real_value rank="0">1e-08</real_value>
                                                    </relative_error>
                                                    <max_iterations>
                                                        <integer_value rank="0">100</integer_value>
                                                    </max_iterations>
                                                    <never_ignore_solver_failures></never_ignore_solver_failures>
                                                    <diagnostics>
                                                        <monitors></monitors>
                                                    </diagnostics>
                                                </solver>
                                                <initial_condition name="WholeMesh">
                                                    <constant>
                                                        <real_value rank="0">1</real_value>
                                                    </constant>
                                                </initial_condition>
                                                <output></output>
                                                <stat></stat>
                                                <detectors>
                                                    <include_in_detectors></include_in_detectors>
                                                </detectors>
                                                <consistent_interpolation></consistent_interpolation>
                                            </prognostic>
                                        </scalar_field>
                                    </angular_moment_set>
                                </parity>
                            </method>
                        </angular_discretisation>
                    </energy_group_set>
                </energy_discretisation>
                <material_data_set name="dataset1">
                    <from_file file_name="rad_1d_3grp_hom_inf_eig.radmats">
                        <number_of_scatter_moments>
                            <integer_value rank="0">1</integer_value>
                        </number_of_scatter_moments>
                        <format_radmats>
                            <maximum_record_length>
                                <integer_value rank="0">132</integer_value>
                            </maximum_record_length>
                        </format_radmats>
                        <physical_material name="fuel">
                            <interpolation_dimension name="first">
                                <interpolation_attribute_none></interpolation_attribute_none>
                                <interpolation_values>
                                    <real_value rank="1" shape="1">1</real_value>
                                </interpolation_values>
                            </interpolation_dimension>
                        </physical_material>
                    </from_file>
                </material_data_set>
                <region_id_material_mapping>
                    <region_to_physical_radiation_material_map name="whole">
                        <region_id>
                            <integer_value rank="1" shape="1">1</integer_value>
                        </region_id>
                        <data_set name="dataset1"></data_set>
                        <physical_material name="fuel"></physical_material>
                    </region_to_physical_radiation_material_map>
                </region_id_material_mapping>
            </particle_type>
        </radiation>
    </embedded_models>
</fluidity_options>
