def read_physics_file(f):
  depth_temp = {}
  data = f.readline().split(",")
  time = float(data[0])
  nLayers = int(data[1])
  depth_temp[float(data[2])] = float(data[3]) # temperature in column 3
  for i in range(nLayers-1):
    data = f.readline().split(",")
    depth_temp[float(data[2])] = float(data[3]) # temperature in column 3
  return depth_temp, time

if not persistent.has_key('temperature_fh'):
