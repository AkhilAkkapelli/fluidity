<?xml version='1.0' encoding='utf-8'?>
<darcy_impes_options>
  <geometry>
    <dimension>
      <integer_value rank="0">1</integer_value>
    </dimension>
    <mesh name="CoordinateMesh">
      <from_file file_name="line">
        <format name="triangle"/>
        <stat>
          <include_in_stat/>
        </stat>
      </from_file>
    </mesh>
    <mesh name="PressureMesh">
      <from_mesh>
        <mesh name="CoordinateMesh"/>
        <mesh_shape>
          <polynomial_degree>
            <integer_value rank="0">1</integer_value>
          </polynomial_degree>
          <element_type>
            <string_value>lagrangian</string_value>
          </element_type>
        </mesh_shape>
        <stat>
          <exclude_from_stat/>
        </stat>
      </from_mesh>
    </mesh>
    <mesh name="ElementWiseMesh">
      <from_mesh>
        <mesh name="CoordinateMesh"/>
        <mesh_shape>
          <polynomial_degree>
            <integer_value rank="0">0</integer_value>
          </polynomial_degree>
          <element_type>
            <string_value>lagrangian</string_value>
          </element_type>
        </mesh_shape>
        <mesh_continuity>
          <string_value>discontinuous</string_value>
        </mesh_continuity>
        <stat>
          <exclude_from_stat/>
        </stat>
      </from_mesh>
    </mesh>
    <quadrature>
      <degree>
        <integer_value rank="0">3</integer_value>
      </degree>
      <controlvolume_surface_degree>
        <integer_value rank="0">3</integer_value>
      </controlvolume_surface_degree>
    </quadrature>
    <disable_geometric_data_cache/>
  </geometry>
  <io>
    <dump_format>
      <string_value>vtk</string_value>
    </dump_format>
    <dump_period_in_timesteps>
      <constant>
        <integer_value rank="0">100</integer_value>
      </constant>
    </dump_period_in_timesteps>
    <output_mesh name="PressureMesh"/>
    <stat>
      <output_at_start/>
    </stat>
  </io>
  <timestepping>
    <current_time>
      <real_value rank="0">0.0</real_value>
    </current_time>
    <timestep>
      <real_value rank="0">2</real_value>
    </timestep>
    <finish_time>
      <real_value rank="0">1000</real_value>
    </finish_time>
  </timestepping>
  <physical_parameters>
    <gravity>
      <magnitude>
        <real_value rank="0">9.8</real_value>
      </magnitude>
      <vector_field name="GravityDirection" rank="1">
        <prescribed>
          <mesh name="ElementWiseMesh"/>
          <value name="WholeMesh">
            <constant>
              <real_value shape="1" dim1="dim" rank="1">1</real_value>
            </constant>
          </value>
        </prescribed>
      </vector_field>
    </gravity>
  </physical_parameters>
  <material_phase name="Phase1">
    <scalar_field name="Pressure" rank="0">
      <prescribed>
        <mesh name="PressureMesh"/>
        <value name="WholeMesh">
          <constant>
            <real_value rank="0">1.0e5</real_value>
          </constant>
        </value>
        <stat>
          <include_cv_stats/>
        </stat>
      </prescribed>
    </scalar_field>
    <scalar_field name="Saturation" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <convergence>
          <include_in_convergence/>
        </convergence>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="RelativePermeability" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <correlation name="PowerLaw">
          <exponents>
            <real_value shape="2" rank="1">2 2</real_value>
          </exponents>
        </correlation>
        <face_value name="FirstOrderUpwind"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="Viscosity" rank="0">
      <prescribed>
        <mesh name="ElementWiseMesh"/>
        <value name="WholeMesh">
          <constant>
            <real_value rank="0">1.725e-5</real_value>
          </constant>
        </value>
        <stat>
          <include_cv_stats/>
        </stat>
      </prescribed>
    </scalar_field>
    <scalar_field name="Density" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <equation_of_state name="IncompressibleLinear">
          <reference_density>
            <real_value rank="0">1.284</real_value>
          </reference_density>
        </equation_of_state>
        <face_value name="FirstOrderUpwind"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <vector_field name="DarcyVelocity" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <boundary_conditions name="no_air_flow_top_sides">
          <surface_ids>
            <integer_value shape="2" rank="1">1 2</integer_value>
          </surface_ids>
          <type name="no_normal_flow"/>
        </boundary_conditions>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </vector_field>
    <scalar_field name="DarcyVelocityOverPorosityCFL" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <vector_field name="TotalDarcyVelocity" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </vector_field>
    <scalar_field name="DivergenceTotalDarcyVelocity" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="Mobility" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="TotalMobility" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="FractionalFlow" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="SumSaturation" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="AveragePressure" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <vector_field name="BulkDarcyVelocity" rank="1">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </vector_field>
  </material_phase>
  <material_phase name="Phase2">
    <scalar_field name="Pressure" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="CapilliaryPressure" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="scalar_python_diagnostic" material_phase_support="multiple">
          <string_value lines="20" type="code" language="python">import numpy
#set the entry capillary pressure
p_ce=1.0
#set the saturation of liquid
sat=states["Phase1"].scalar_fields["Saturation"]
#set the field
for n in range(field.node_count):
  sat_n=sat.node_val(n)
  p_c=p_ce/((sat_n)**0.5)
  field.set(n,p_c)</string_value>
        </algorithm>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="Saturation" rank="0">
      <prognostic>
        <mesh name="PressureMesh"/>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value rank="0">0.03125</real_value>
          </constant>
        </initial_condition>
        <convergence>
          <include_in_convergence/>
        </convergence>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
    <scalar_field name="RelativePermeability" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="Viscosity" rank="0">
      <prescribed>
        <mesh name="ElementWiseMesh"/>
        <value name="WholeMesh">
          <constant>
            <real_value rank="0">1.0e-3</real_value>
          </constant>
        </value>
        <stat>
          <include_cv_stats/>
        </stat>
      </prescribed>
    </scalar_field>
    <scalar_field name="Density" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <equation_of_state name="IncompressibleLinear">
          <reference_density>
            <real_value rank="0">1000</real_value>
          </reference_density>
        </equation_of_state>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <vector_field name="DarcyVelocity" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <boundary_conditions name="liquid_top_flow">
          <surface_ids>
            <integer_value shape="1" rank="1">1</integer_value>
          </surface_ids>
          <type name="prescribed_normal_flow">
            <constant>
              <real_value rank="0">-1.5e-05</real_value>
            </constant>
          </type>
        </boundary_conditions>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </vector_field>
    <scalar_field name="DarcyVelocityOverPorosityCFL" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="Mobility" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="FractionalFlow" rank="0">
      <diagnostic>
        <mesh name="PressureMesh"/>
        <algorithm name="Internal"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="theta_s">
      <diagnostic>
        <mesh name="ElementWiseMesh"/>
        <algorithm name="scalar_python_diagnostic" material_phase_support="multiple">
          <string_value lines="20" type="code" language="python">#extract the Immobile saturation
sats=states["Phase2"].scalar_fields["ImmobileSaturation"]
#extract the porosity
prt=states["Phase1"].scalar_fields["Porosity"]

for n in range (field.node_count):
  sats_n=sats.node_val(n)
  prt_n=prt.node_val(n)
  thetas_n=sats_n*prt_n
  field.set(n,thetas_n)</string_value>
          <depends>
            <string_value lines="1">Phase2::ImmobileSaturation, Phase1::Porosity</string_value>
            <comment>calculate the Immobile saturation first, so that 'theta=Sat_i*porosity' will be calculated at the current time, otherwise theta will be the value at the previous timestep.</comment>
          </depends>
        </algorithm>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="C_d_elem">
      <diagnostic>
        <mesh name="ElementWiseMesh"/>
        <algorithm source_field_type="scalar" material_phase_support="single" name="scalar_galerkin_projection" source_field_name="C_d"/>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <scalar_field name="C_d">
      <prognostic>
        <mesh name="PressureMesh"/>
        <solver name="DefaultSolverOptionsGMRESSOR">
          <iterative_method name="gmres">
            <restart>
              <integer_value rank="0">30</integer_value>
            </restart>
          </iterative_method>
          <preconditioner name="sor"/>
          <relative_error>
            <real_value rank="0">1.0e-10</real_value>
          </relative_error>
          <max_iterations>
            <integer_value rank="0">1000</integer_value>
          </max_iterations>
          <never_ignore_solver_failures/>
        </solver>
        <face_value name="FiniteElement">
          <limit_face_value>
            <limiter name="Sweby"/>
          </limit_face_value>
        </face_value>
        <initial_condition name="WholeMesh">
          <constant>
            <real_value rank="0">0.0</real_value>
          </constant>
        </initial_condition>
        <boundary_conditions name="Delta">
          <surface_ids>
            <integer_value shape="1" rank="1">1</integer_value>
          </surface_ids>
          <type name="dirichlet">
            <apply_weakly/>
            <python>
              <string_value lines="20" type="code" language="python">def val(X, t):
  if t&lt;1.1:
    bc=1.0e+04
  else:
    bc=0.0
  #print 'time', t
  #print 'bc', bc
  return bc</string_value>
            </python>
          </type>
        </boundary_conditions>
        <tensor_field name="Diffusivity" rank="2">
          <prescribed>
            <mesh name="ElementWiseMesh"/>
            <value name="WholeMesh">
              <isotropic>
                <constant>
                  <real_value rank="0">3.14e-05</real_value>
                </constant>
              </isotropic>
            </value>
          </prescribed>
        </tensor_field>
        <scalar_field name="Source" rank="0">
          <diagnostic>
            <mesh name="ElementWiseMesh"/>
            <algorithm name="scalar_python_diagnostic" material_phase_support="multiple">
              <string_value lines="20" type="code" language="python">#extract alpha
alpha=states["Phase2"].scalar_fields["MassTransferCoefficient"]
#extract old c_d
cd_old=states["Phase2"].scalar_fields["OldC_d_elem"]
#cd=states["Phase2"].scalar_fields["C_d_elem"]

#print "maxOldC_d_elem", max(cd_old)
#print "maxC_d_elem", max(cd)

#extract the old C_s
cs_old=states["Phase2"].scalar_fields["OldC_s"]

#set the field
for n in range(field.node_count):
  cs_old_n=cs_old.node_val(n)
  cd_old_n=cd_old.node_val(n)
  alpha_n=alpha.node_val(n)
  
  src=-alpha_n*(cd_old_n-cs_old_n)
  field.set(n,src)</string_value>
            </algorithm>
            <stat>
              <include_cv_stats/>
            </stat>
            <consistent_interpolation/>
          </diagnostic>
        </scalar_field>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
    <scalar_field name="C_s">
      <diagnostic>
        <mesh name="ElementWiseMesh"/>
        <algorithm name="scalar_python_diagnostic" material_phase_support="multiple">
          <string_value lines="20" type="code" language="python">#extract alpha
alpha=states["Phase2"].scalar_fields["MassTransferCoefficient"]
#extract old c_d
cd_old=states["Phase2"].scalar_fields["OldC_d_elem"]
#extract the old C_s
cs_old=states["Phase2"].scalar_fields["OldC_s"]
#extract old theta_s
thetas_old=states["Phase2"].scalar_fields["Oldtheta_s"]
#extract new theta_s
thetas=states["Phase2"].scalar_fields["theta_s"]
#print "max oldthetas", max(thetas_old)
#print "max thetas", max(thetas)
#print "time", time
#print "cs", max(cs_old)

for n in range (field.node_count):

    cs_old_n=cs_old.node_val(n)
    cd_old_n=cd_old.node_val(n)
    alpha_n=alpha.node_val(n)
    thetas_old_n=thetas_old.node_val(n)
    thetas_n=thetas.node_val(n)
    tac=dt*alpha_n*(cd_old_n-cs_old_n)
    tactc=tac+thetas_old_n*cs_old_n
    
    cs=tactc/thetas_n
    
    field.set(n,cs)</string_value>
          <depends>
            <string_value lines="1">theta_s</string_value>
          </depends>
        </algorithm>
        <stat>
          <include_cv_stats/>
        </stat>
        <consistent_interpolation/>
      </diagnostic>
    </scalar_field>
    <MobileImmobileModel>
      <scalar_field name="ImmobileSaturation" rank="0">
        <prescribed>
          <mesh name="PressureMesh"/>
          <value name="WholeMesh">
            <constant>
              <real_value rank="0">0.0035</real_value>
            </constant>
          </value>
          <stat>
            <include_cv_stats/>
          </stat>
        </prescribed>
      </scalar_field>
      <scalar_field name="MobileSaturation" rank="0">
        <diagnostic>
          <mesh name="PressureMesh"/>
          <algorithm name="Internal"/>
          <stat>
            <include_cv_stats/>
          </stat>
          <consistent_interpolation/>
        </diagnostic>
      </scalar_field>
      <scalar_field name="MassTransferCoefficient" rank="0">
        <prescribed>
          <mesh name="PressureMesh"/>
          <value name="WholeMesh">
            <constant>
              <real_value rank="0">4.29e-06</real_value>
            </constant>
          </value>
          <stat>
            <include_cv_stats/>
          </stat>
        </prescribed>
      </scalar_field>
    </MobileImmobileModel>
  </material_phase>
  <porous_media>
    <scalar_field name="Porosity">
      <prescribed>
        <mesh name="ElementWiseMesh"/>
        <value name="WholeMesh">
          <constant>
            <real_value rank="0">0.4</real_value>
          </constant>
        </value>
        <stat>
          <include_cv_stats/>
        </stat>
      </prescribed>
    </scalar_field>
    <scalar_field name="AbsolutePermeability">
      <prescribed>
        <mesh name="ElementWiseMesh"/>
        <value name="WholeMesh">
          <constant>
            <real_value rank="0">1.567346939e-9</real_value>
          </constant>
        </value>
        <stat>
          <include_cv_stats/>
        </stat>
      </prescribed>
    </scalar_field>
  </porous_media>
</darcy_impes_options>
