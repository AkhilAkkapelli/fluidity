            <preserve_mesh_regions></preserve_mesh_regions>
            <vertically_structured_adaptivity>
                <inhomogenous_vertical_resolution></inhomogenous_vertical_resolution>
                <vertically_align_metric></vertically_align_metric>
            </vertically_structured_adaptivity>
        </hr_adaptivity>
    </mesh_adaptivity>
