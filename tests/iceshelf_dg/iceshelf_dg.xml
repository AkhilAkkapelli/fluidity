<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE testproblem SYSTEM "regressiontest.dtd">

<testproblem>
  <name>iceshelf_dg</name>
  <owner userid="asc"/>
  <tags>flml</tags>
  <problem_definition length="short" nprocs="1">
    <command_line>fluidity -v3 -l shelf2d.flml</command_line>
  </problem_definition>
  <variables>
    <variable name="ElapsedTime" language="python">from fluidity_tools import stat_parser 
s = stat_parser("iceshelf_dg.stat")
elapsedtime=s["ElapsedTime"]["value"][-1]</variable>
  </variables>
  <pass_tests>
    <test name="endtime" language="python">assert(elapsedtime - 1.0 &lt; 1E-10)</test>
  </pass_tests>
  <warn_tests>
  </warn_tests>
</testproblem>
