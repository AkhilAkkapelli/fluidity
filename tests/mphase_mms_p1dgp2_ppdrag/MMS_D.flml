            <mesh name="VelocityMesh"/>
            <value name="WholeMesh">
              <python>
                <string_value lines="20" type="code" language="python">def val(XX, t):
    from math import sin,cos,sqrt
    mu = 0.3;
    rho = 1.0;
    x = XX[0];
    y = XX[1];
    vfrac = 0.8
    vfrac_p1 = 0.05
    vfrac_p2 = 0.15
    dvfrac_x = 0.0
    dvfrac_y = 0.0
    
    Su = -(x*cos(y) - 0.500000000000000*cos(x)*cos(y))*mu*vfrac - ((-0.250000000000000*sin(x)*cos(y) - cos(y))*(x*cos(y) - 0.250000000000000*cos(x)*cos(y)) - (x*sin(y) - 0.250000000000000*sin(y)*cos(x))*sin(y))*rho*vfrac - vfrac*sin(x)*cos(y) - ((-0.250000000000000*sin(x)*cos(y) - cos(y))*dvfrac_x + (x*sin(y) - 0.250000000000000*sin(y)*cos(x))*dvfrac_y)*mu
    Sv = rho*vfrac*sin(y)*cos(y) - dvfrac_y*mu*cos(y) + mu*vfrac*sin(y) - vfrac*sin(y)*cos(x) + 9.80000000000000*rho*vfrac
    
    # Fluid-particle drag
    mu_f = mu
    d_p1 = 1.0
    d_p2 = 0.1
    Du = -18.0000000000000*(x*cos(y) + sin(x)*cos(y) - 0.250000000000000*cos(x)*cos(y))*mu_f*vfrac_p1/d_p1**2 - 18.0000000000000*(x*cos(y) + 0.750000000000000*cos(x)*cos(y))*mu_f*vfrac_p2/d_p2**2
    Dv = -18.0000000000000*(sin(x)*sin(y) - sin(y)*cos(x) - sin(y))*mu_f*vfrac_p1/d_p1**2 - 18.0000000000000*(2*sin(x)*sin(y) - sin(y))*mu_f*vfrac_p2/d_p2**2
    
    # Divide the result through by rho to cancel out
    # the multiplication by rho in the assembly.
    return ((Su+Du)/rho, (Sv+Dv)/rho)</string_value>
