  agent['NutrientUptake'] = 0.005
  agent['Nutrient'] += agent['NutrientIngested']
  print "ml805 nut " + str(agent['Nutrient']) + " Ing: " + str(agent['NutrientIngested'])
  
  # Per-individual detritus release
  agent['DetritusRelease'] = min(0.005, agent['Detritus'])
  agent['Detritus'] -= agent['DetritusRelease']
  return agent</string_value>
