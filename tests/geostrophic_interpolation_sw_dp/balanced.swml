                        <integer_value rank="1" shape="1">4</integer_value>
                    </physical_boundary_ids>
                    <aliased_boundary_ids>
                        <integer_value rank="1" shape="1">3</integer_value>
                    </aliased_boundary_ids>
                    <coordinate_map>
                        <string_value lines="20" type="python">def val(x, t):&#x0A;&#x09;return [x[0], x[1]-5.0e5]</string_value>
                    </coordinate_map>
                </periodic_boundary_conditions>
                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_mesh>
        </mesh>
        <mesh name="Adapt1">
            <from_file file_name="square-x2">
                <format name="triangle"></format>
                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_file>
            <exclude_from_mesh_adaptivity></exclude_from_mesh_adaptivity>
        </mesh>
        <mesh name="Adapt2">
