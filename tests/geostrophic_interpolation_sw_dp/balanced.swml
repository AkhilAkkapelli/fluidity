                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_file>
            <exclude_from_mesh_adaptivity></exclude_from_mesh_adaptivity>
        </mesh>
        <mesh name="OutputMesh">
            <from_mesh>
                <mesh name="CoordinateMesh"></mesh>
                <mesh_shape>
                    <polynomial_degree>
                        <integer_value rank="0">2</integer_value>
                    </polynomial_degree>
                </mesh_shape>
                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_mesh>
        </mesh>
        <mesh name="InitialMesh">
            <from_file file_name="square">
                <format name="triangle"></format>
                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_file>
            <exclude_from_mesh_adaptivity></exclude_from_mesh_adaptivity>
        </mesh>
        <quadrature>
            <degree>
                <integer_value rank="0">4</integer_value>
            </degree>
        </quadrature>
    </geometry>
    <io>
