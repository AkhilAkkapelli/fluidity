    </theta>
    <nonlinear_iterations>
      <integer_value rank="0">1</integer_value>
    </nonlinear_iterations>
    <finish_time>
      <real_value rank="0">19.989208802178716</real_value>
      <comment>speed = 2*pi/(1+8*pi**2)
distance = 2*pi
time = distance/speed = (1+8*pi**2)
divided by 4</comment>
    </finish_time>
  </timestepping>
  <physical_parameters>
    <gravity>
      <magnitude>
        <real_value rank="0">1.0e4</real_value>
        <comment>g &lt;=&gt; 1/e, e = 1.0e-4</comment>
      </magnitude>
      <vector_field name="GravityDirection" rank="1">
        <prescribed>
