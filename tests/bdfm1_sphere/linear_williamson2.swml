        <initial_condition name="WholeMesh">
          <python>
            <string_value lines="20" type="python">def val(X,t):
  from math import pi
  x = X[0]
  y = X[1]
  z = X[2]
  R0 = 6.37122e6
  R = (x**2+y**2+z**2)**0.5
  Rh = (x**2+y**2)**0.5
  cosphi = Rh/R
  u0 = 2*pi*R0/(12.*24.*60.*60.)
  if(Rh==0):
    return [0,0,0]
  else:
    return [-u0*cosphi*y/Rh,u0*cosphi*x/Rh,0]</string_value>
          </python>
        </initial_condition>
