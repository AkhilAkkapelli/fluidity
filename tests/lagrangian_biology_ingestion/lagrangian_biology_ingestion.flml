              <algorithm name="Internal" material_phase_support="multiple"/>
              <output/>
              <stat/>
              <detectors>
                <include_in_detectors/>
              </detectors>
            </diagnostic>
            <stage_diagnostic/>
            <include_in_io/>
          </scalar_field>
          <scalar_field name="Depletion">
            <diagnostic>
              <algorithm name="Internal" material_phase_support="multiple"/>
              <output/>
              <stat/>
              <detectors>
                <include_in_detectors/>
              </detectors>
            </diagnostic>
          </scalar_field>
          <scalar_field name="IngestedCells">
            <diagnostic>
              <algorithm name="Internal" material_phase_support="multiple"/>
              <output/>
              <stat/>
              <detectors>
                <include_in_detectors/>
              </detectors>
            </diagnostic>
            <include_in_io/>
          </scalar_field>
          <integrate_along_path/>
          <food_type name="Inactive"/>
        </food_set>
        <scalar_field name="Agents">
          <diagnostic>
            <algorithm name="Internal" material_phase_support="multiple"/>
            <output/>
            <stat/>
            <detectors>
              <include_in_detectors/>
            </detectors>
          </diagnostic>
        </scalar_field>
      </functional_group>
    </lagrangian_ensemble_biology>
  </embedded_models>
