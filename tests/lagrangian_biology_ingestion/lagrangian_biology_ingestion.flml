<?xml version='1.0' encoding='utf-8'?>
<fluidity_options>
  <simulation_name>
    <string_value lines="1">ingestion_2d</string_value>
  </simulation_name>
  <problem_type>
    <string_value lines="1">oceans</string_value>
  </problem_type>
  <geometry>
    <dimension>
      <integer_value rank="0">2</integer_value>
    </dimension>
    <mesh name="CoordinateMesh">
      <from_file file_name="src/square">
        <format name="gmsh"/>
        <stat>
          <include_in_stat/>
        </stat>
      </from_file>
    </mesh>
    <mesh name="DGMesh">
      <from_mesh>
        <mesh name="CoordinateMesh"/>
        <mesh_shape>
          <polynomial_degree>
            <integer_value rank="0">1</integer_value>
          </polynomial_degree>
        </mesh_shape>
        <mesh_continuity>
          <string_value>discontinuous</string_value>
        </mesh_continuity>
        <stat>
          <exclude_from_stat/>
        </stat>
      </from_mesh>
    </mesh>
    <quadrature>
      <degree>
        <integer_value rank="0">4</integer_value>
      </degree>
    </quadrature>
  </geometry>
  <io>
    <dump_format>
      <string_value>vtk</string_value>
    </dump_format>
    <dump_period>
      <constant>
        <real_value rank="0">1</real_value>
      </constant>
    </dump_period>
    <output_mesh name="CoordinateMesh"/>
    <stat>
      <output_at_start/>
    </stat>
  </io>
  <timestepping>
    <current_time>
      <real_value rank="0">0</real_value>
    </current_time>
    <timestep>
      <real_value rank="0">1.0</real_value>
    </timestep>
    <finish_time>
      <real_value rank="0">20.0</real_value>
    </finish_time>
  </timestepping>
  <material_phase name="Water">
    <vector_field name="Velocity" rank="1">
      <prescribed>
        <mesh name="CoordinateMesh"/>
        <value name="WholeMesh">
          <constant>
            <real_value shape="2" dim1="dim" rank="1">0.0 0.0</real_value>
          </constant>
        </value>
        <output/>
        <stat>
          <include_in_stat/>
        </stat>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
      </prescribed>
    </vector_field>
  </material_phase>
  <embedded_models>
    <lagrangian_ensemble_biology>
      <random_seed>
        <integer_value rank="0">123456</integer_value>
      </random_seed>
      <functional_group name="Prey">
        <variables>
          <state_variable name="Stage"/>
          <state_variable name="Size">
            <scalar_field name="Ensemble">
              <diagnostic>
                <algorithm name="Internal" material_phase_support="multiple"/>
                <output/>
                <stat/>
                <detectors>
                  <include_in_detectors/>
                </detectors>
              </diagnostic>
              <stage_diagnostic/>
            </scalar_field>
          </state_variable>
          <chemical_variable name="Carbon">
            <scalar_field name="Particulate">
              <diagnostic>
                <algorithm name="Internal" material_phase_support="multiple"/>
                <output/>
                <stat/>
                <detectors>
                  <include_in_detectors/>
                </detectors>
                <do_not_recalculate/>
              </diagnostic>
              <stage_diagnostic/>
            </scalar_field>
            <include_in_io/>
          </chemical_variable>
        </variables>
        <initial_state>
          <number_of_agents>
            <integer_value rank="0">10000</integer_value>
          </number_of_agents>
          <position>
            <string_value lines="20" type="code" language="python">def val(t):
  import numpy as np
  x = np.reshape([np.arange(0.005, 1., 0.01) for i in range(100)], (1,10000))
  y = np.reshape([np.ones(100)*i for i in np.arange(0.005, 1., 0.01)], (1,10000))
  positions = np.reshape(np.concatenate((x,y)),(2,10000)).T
  return positions</string_value>
          </position>
          <biology>
            <string_value lines="20" type="code" language="python">from lebiology import stage_id

def val(agent):
  agent['Stage'] = stage_id('Prey', 'Inactive')
  agent['Size'] = 100.0
  agent['Carbon'] = 1.e-6
  return agent</string_value>
          </biology>
        </initial_state>
        <movement>
          <subcycles>
            <integer_value rank="0">1</integer_value>
          </subcycles>
          <search_tolerance>
            <real_value rank="0">1.0e-10</real_value>
          </search_tolerance>
          <reflect_on_boundary/>
          <parametric_guided_search/>
        </movement>
        <environment/>
        <stages>
          <stage name="Inactive">
            <biology>
              <python>
                <string_value lines="20" type="code" language="python">def val(agent, env, dt):
  return agent</string_value>
              </python>
            </biology>
          </stage>
        </stages>
        <scalar_field name="Agents">
          <diagnostic>
            <algorithm name="Internal" material_phase_support="multiple"/>
            <output/>
            <stat/>
            <detectors>
              <include_in_detectors/>
            </detectors>
          </diagnostic>
        </scalar_field>
      </functional_group>
      <functional_group name="Predator">
        <variables>
          <state_variable name="Stage"/>
          <state_variable name="Size">
            <scalar_field name="Ensemble">
              <diagnostic>
                <algorithm name="Internal" material_phase_support="multiple"/>
                <output/>
                <stat/>
                <detectors>
                  <include_in_detectors/>
                </detectors>
              </diagnostic>
              <stage_diagnostic/>
            </scalar_field>
          </state_variable>
          <chemical_variable name="Carbon">
            <scalar_field name="Particulate">
              <diagnostic>
                <algorithm name="Internal" material_phase_support="multiple"/>
                <output/>
                <stat/>
                <detectors>
                  <include_in_detectors/>
                </detectors>
                <do_not_recalculate/>
              </diagnostic>
            </scalar_field>
            <scalar_field name="Ingested">
              <diagnostic>
                <algorithm name="Internal" material_phase_support="multiple"/>
                <output/>
                <stat/>
                <detectors>
                  <include_in_detectors/>
                </detectors>
                <do_not_recalculate/>
              </diagnostic>
            </scalar_field>
            <include_in_io/>
          </chemical_variable>
        </variables>
        <initial_state>
          <number_of_agents>
            <integer_value rank="0">1000</integer_value>
          </number_of_agents>
          <position>
            <string_value lines="20" type="code" language="python">def val(t):
  from numpy import concatenate, reshape, random
  x = random.uniform(0.,1., 1000)
  y = random.uniform(0.,1., 1000)
  positions = reshape(concatenate((x,y)),(2,1000)).T
  return positions</string_value>
          </position>
          <biology>
            <string_value lines="20" type="code" language="python">from lebiology import stage_id

def val(agent):
  agent['Stage'] = stage_id('Predator', 'Active')
  agent['Size'] = 1.0
  agent['Carbon'] = 0.0
  return agent</string_value>
          </biology>
        </initial_state>
        <movement>
          <subcycles>
            <integer_value rank="0">1</integer_value>
          </subcycles>
          <search_tolerance>
            <real_value rank="0">1.0e-10</real_value>
          </search_tolerance>
          <reflect_on_boundary/>
          <geometric_tracking/>
          <random_walk name="RandomDisplacement">
            <python>
              <string_value lines="20" type="code" language="python">from numpy import random

def val(position, vars, dt):
  v = random.uniform(-0.5,0.5, 2)
  #v = random.uniform(0.,1., 2)
  #v = (v - position)
  return v</string_value>
            </python>
          </random_walk>
        </movement>
        <environment/>
        <stages>
          <stage name="Active">
            <biology>
              <python>
                <string_value lines="20" type="code" language="python">from __builtin__ import str

def val(agent, env, dt):
  #print "ml805 Pred env: " + str(env)
  #print "ml805 Pred agent b4: " + str(agent)
  agent['Carbon'] = agent['Carbon'] + agent['CarbonIngested']
  agent['FoodRequest']['Inactive'] = 100. * dt
  return agent</string_value>
              </python>
            </biology>
          </stage>
        </stages>
        <food_set functional_group="Prey" name="Food">
          <scalar_field name="Concentration">
            <diagnostic>
              <algorithm name="Internal" material_phase_support="multiple"/>
              <output/>
              <stat/>
              <detectors>
                <include_in_detectors/>
              </detectors>
            </diagnostic>
          </scalar_field>
          <scalar_field name="Request">
