layer=None
time=0.0

# store a filehandle in persistent dict
if not persistent.has_key('velocity_mld_fh'):
  persistent['velocity_mld_fh'] = open("vew_tm_mld.csv" , 'r')

def layerheight(t):
  # read physics
  data = persistent['velocity_mld_fh'].readline().split(",")
  if not t==float(data[0]):
    raise RuntimeError('Velocity: Wrong t in read_physics: ' + data[0])
  return float(data[1])
  
def val(X,t):
  global layer, time
  if t &gt; time or layer==None:
    time = t
    layer=layerheight(t)
  
  if abs(X[0])&lt;=layer:
    vel_Z=0.0
  else:
    # sinking velocity of 4cm per hour
    vel_Z=-0.000011111
  return [vel_Z]</string_value>
