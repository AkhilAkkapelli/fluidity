                  <string_value lines="20" type="python">global calc_mu, get_spline, get_layer, read_physics_file

time = 0.0
spline = None
layer = None

if not persistent.has_key('diffusivity_mld_fh'):
  persistent['diffusivity_mld_fh'] = open("vew_tm_mld.csv", 'r')
  
def get_layer(t):
  # read physics
  data = persistent['diffusivity_mld_fh'].readline().split(",")
  if not t==float(data[0]):
    raise RuntimeError('PrescribedDiffusivity: Wrong t in read_physics: ' + data[0])
  return float(data[1])  

def get_spline(t):
  from numpy import arange
  from scipy import interpolate
  layer = get_layer(t)

  # create spline
  height = arange(0.0, 300.0, 10.0)
  mu = [calc_mu(z, layer) for z in height]
  tck = interpolate.splrep(height,mu)
  return tck
    
def calc_mu(z, layer):
  interfacewidth=30.0
  mu_MLD=0.3  #0.05
  if abs(z)&lt;=layer-interfacewidth:
    mu_Z=mu_MLD
  elif abs(z)&lt;=layer:
    beta=(abs(z)-(layer-interfacewidth))/interfacewidth
    mu_Z=(1-beta)*mu_MLD
  else:
    mu_Z=1e-5
  return mu_Z
  
def val(X,t):
  from scipy import interpolate
  global time, spline, layer
  #if t &gt; time or spline==None:
  if t &gt; time or layer == None:
    time = t
    #spline = get_spline(t)
    layer = get_layer(t)
  
  mu = calc_mu(abs(X[0]), layer)
  #mu = interpolate.splev(abs(X[0]), spline)
  return [[mu]]</string_value>
