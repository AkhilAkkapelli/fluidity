def read_physics_file(f):  
  depth_irrad = {}
  data = f.readline().split(",")
  time = float(data[0])
  nLayers = int(data[1])
  depth_irrad[float(data[2])] = float(data[4]) # irradiance in column 4
  for i in range(nLayers-1):
    data = f.readline().split(",")
    depth_irrad[float(data[2])] = float(data[4]) # irradiance in column 4
  return depth_irrad, time

if not persistent.has_key('irradiance_fh'):
