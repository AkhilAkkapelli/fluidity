def val(X,t):
  if not t==time:
    raise RuntimeError('Irradiance: Wrong t in read_physics: ' + str(time) + ' expected: ' + str(t))
  
  if abs(X[0]) &lt;= 499:
    z = abs(X[0])
  else:
    z = 499
  return depth_irrad[z]</string_value>
