time_variable_python_interface =
    ## Determines which time variable is available in the 
    ## Python interface: The current time (default) or 
    ## the current timestep.
    element time_variable_in_python
    {
        ## Passes the current simulation time to the python 
        ## interface below.
        element current_time {empty}
        |
        ## Passes the current timestep to the python 
        ## interface below.
        element current_timestep {empty}
    }

apply_velocity_to_solid_mesh =
#    ## Determines whether the given velocity is actually applied
#    ## to the solid mesh or not:
(
        ## Choose whether the given velocity is applied to the solid
        ## mesh. This results in a moving solid mesh.
        element move_solid_mesh {empty}
        |
        ## Choose whether the given velocity is applied to the solid
        ## mesh. This option applies the given velocity to a solid, 
        ## but the solid geometry/mesh stays stationary. This can 
        ## be useful for e.g. symmetrical and rotational bodies such
        ## as a tyre deformed tyre, which deformation should
        ## remain constant in time.
       element stationary_solid_mesh {empty}
)
